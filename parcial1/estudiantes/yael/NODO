class Nodo:
    def __init__(self, valor):
        self.valor = valor
        self.siguiente = None

class Lista:
    def __init__(self):
        self.cabeza = None

    def append(self, valor):
        nuevo_nodo = Nodo(valor)
        if self.cabeza is None:
            self.cabeza = nuevo_nodo
        else:
            aux = self.cabeza
            while aux.siguiente is not None:
                aux = aux.siguiente
            aux.siguiente = nuevo_nodo

    def get(self, pos):
        if self.cabeza is None:
            return None
        current = self.cabeza
        for _ in range(pos):
            if current.siguiente is None:
                return None
            current = current.siguiente
        return current.valor

if __name__ == '__main__':
    p = int(input())
    n = int(input())
    l = Lista()
    for _ in range(n):
        l.append(int(input()))

    print(l.get(p))
